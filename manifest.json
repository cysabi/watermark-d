{
    "manifest_version": 3,
    "name": "drought-gpt",
    "description": "track how much water you are using through gpt",
    "version": "1",
    "icons": {
        "128": "icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://chatgpt.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["content.css", "icon-128.png"],
            "matches": []
        }
    ]
}
